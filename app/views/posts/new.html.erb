<h2>create a new Post</h2>
<%= form_with model: @post, url: user_posts_path, method: :post do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title, placeholder: 'Title' %><br><br>
  <%= form.label :Text %>
  <%= form.text_area :text, placeholder: 'Text' %><br><br>
  <%= form.submit 'Create post'%>
<% end %>